generator client {
  provider = "prisma-client-js"
}

datasource qrs {
  provider = "mysql"
  url      = env("LOCALHOST_DATABSE_URL")
}

model t_device_data {
  group_id    String    @qrs.VarChar(50)
  id          Int       @id
  user_id     String?   @qrs.VarChar(255)
  co2         String?   @qrs.VarChar(255)
  tvoc        String?   @qrs.VarChar(255)
  pm2_5       String?   @qrs.VarChar(255)
  pm10        String?   @qrs.VarChar(255)
  hud         String?   @qrs.VarChar(255)
  temper      String?   @qrs.VarChar(255)
  dc          String?   @qrs.VarChar(255)
  update_time DateTime? @qrs.DateTime(0)
  ch20        String?   @qrs.VarChar(255)
}
